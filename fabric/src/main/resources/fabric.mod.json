{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${version}",
  "name": "${mod_name}",
  "description": "${description}",
  "authors": [
    "${mod_author}"
  ],
  "contact": {
    "homepage": "https://faewulf.xyz",
    "sources": "https://github.com/FaeWulf/Diversity",
    "issues": "https://github.com/FaeWulf/Diversity/issues"
  },
  "license": "${license}",
  "icon": "${mod_id}.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "xyz.faewulf.diversity.Diversity"
    ],
    "modmenu": [
      "xyz.faewulf.diversity.config.ModMenuIntegration"
    ]
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.discord": "https://discord.gg/xZneCTcEvb"
      },
      "update_checker": true
    }
  },
  "mixins": [
    "compat.mixins.json",
    "client.mixins.json",
    "${mod_id}.mixins.json",
    "${mod_id}.fabric.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${fabric_loader_version}",
    "fabric-api": "*",
    "minecraft": "${minecraft_version}",
    "java": ">=${java_version}"
  }
}
  